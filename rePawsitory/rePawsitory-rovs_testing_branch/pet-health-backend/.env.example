# Environment Variables Example
# Copy this file to .env and fill in your values (for local development)

# MongoDB Connection String
MONGODB_URI=mongodb+srv://your-username:your-password@your-cluster.mongodb.net/?appName=YourApp

# Server Port
PORT=5001

# Environment
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# JWT Secret (add a random secret key for JWT tokens)
JWT_SECRET=your-super-secret-jwt-key-here
